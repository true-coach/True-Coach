@{
    ViewData["Title"] = "Home Page";
    Layout = "_Layout";

}

<div class="text-center">
    <h1 class="display-4">Welcome @ViewBag.UserName</h1>
</div>

<div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-50 h-50" src="~/css/Assets/running-runner-long-distance-fitness-40751.jpeg" alt="First slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-50 h-50" src="~/css/Assets/pexels-photo-1112633.jpeg" alt="Second slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-50" src="~/css/Assets/Personal_Trainer1.jpg" alt="Third slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-50 h-50" src="~/css/Assets/asphalt-clouds-endurance-421160.jpg" alt="Fourth slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-50 h-50" src="~/css/Assets/Personal-Training2.jpg" alt="Fifth slide">
        </div>
    </div>
</div>
